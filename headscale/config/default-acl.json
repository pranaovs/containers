{
  "groups": {
    "group:admin": [
      "personal@"
    ],
    "group:internet": [
      "personal@"
    ]
  },
  "hosts": {
    "devbox": "100.64.0.1",
    "devserver": "100.64.0.2"
  },
  "tagOwners": {
    "tag:server": [
      "personal@"
    ]
  },
  "acls": [
    {
      "action": "accept",
      "src": [
        "group:admin"
      ],
      "dst": [
        "*:*"
      ]
    },
    {
      "action": "accept",
      "src": [
        "group:internet"
      ],
      "dst": [
        "autogroup:internet:*"
      ]
    },
    {
      "action": "accept",
      "src": [
        "tag:server"
      ],
      "dst": [
        "devbox:8096"
      ]
    }
  ],
  "ssh": [
    {
      "action": "accept",
      "src": [
        "personal@"
      ],
      "dst": [
        "*"
      ],
      "users": [
        "personal",
        "root"
      ]
    }
  ]
}
